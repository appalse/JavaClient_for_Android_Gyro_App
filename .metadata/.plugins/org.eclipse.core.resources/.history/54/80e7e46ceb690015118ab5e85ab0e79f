import java.net.*;
import java.io.*;

public class ClientMain {
	private static String serverIpAddress = "192.168.56.1"; 
	private static int serverPort = 12345;
	public static void main(String[] args) {
		InetAddress ipAddress = InetAddress.getByName(serverIpAddress);
		try{
			Socket s = new Socket(serverIpAddress, serverPort);
			String text = "hello server";
			s.getOutputStream().write(text.getBytes());
		}catch(Exception e) {
			e.printStackTrace();
		}
		

	}

}
